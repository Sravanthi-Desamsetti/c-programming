
#include <stdio.h>
int main() 
{
    int x,ans;
    printf("Enter a number:");
    scanf("%d",&x);
    ans=checkevenodd(x);
    if(ans==0)
    printf("EVEN");
    else
    printf("odd");
}
int checkevenodd(int x)
{
    if(x%2==0)
    return (0);
    else
    return (1);
}
output:
   
Enter a number:85
odd
