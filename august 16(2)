#include <stdio.h>
int main()
{
  int i,j,arr[5],flag;
  printf("Enter elements into array:\n");
  for(i=0;i<5;i++)
  scanf("%d ",&arr[i]);
  printf("The prime numbers of given array are:");
  for(i=0;i<5;i++)
  {
      flag=0;
      for(j=2;j<arr[i];j++)
      {
          if(arr[i]%j==0)
          {
              flag=1;
              break;
          }
      }
      if(flag==0)
      printf("%d ",arr[i]);
  }
    return 0;
}

output:Enter elements into array:
5
6
9
3
7
1
0
The prime numbers of given array are:5 3 1 
